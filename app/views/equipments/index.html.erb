<div class="container">
  <h1>Equipments#index</h1>
  <p>Find me in app/views/equipments/index.html.erb</p>

  <ul>
    <% @equipments.each do |equipment| %>
    <li>
      <div class="photos">
       <% equipment.photos.each do |photo| %>
        <%= cl_image_tag photo.key%>
         <% end %>
      </div>
      <div>
         <%= link_to equipment.name, equipment_path(equipment) %> owned by <strong><%= User.find(equipment.user_id).first_name %></strong>
       <% end %>
      </div>
    </li>
  </ul>

  <%= link_to "Add Equipment", new_equipment_path %>
</div>
